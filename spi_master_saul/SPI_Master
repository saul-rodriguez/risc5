    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x5620660d9940_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
