v0x5620660fc7e0_0 .var "tx_ready", 0 0;
v0x5620660fc8a0_0 .var "tx_start", 0 0;
v0x5620660fc960_0 .net "wdata", 7 0, v0x5620660fd410_0;  1 drivers
v0x5620660fca40_0 .net "wen", 0 0, v0x5620660fd4e0_0;  1 drivers
    .scope S_0x5620660fbe00;
T_1 ;
    %wait E_0x5620660cc760;
    %load/vec4 v0x5620660fc560_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5620660fc620_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660d6c00_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5620660fc7e0_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5620660fc700_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660fc8a0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5620660d47d0_0;
    %load/vec4 v0x5620660d9d60_0;
    %pushi/vec4 3402284734, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x5620660d7f40_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0 T_1.4, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_1.5, 8;
T_1.4 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_1.5, 8;
 ; End of false expr.
    %blend;
T_1.5;
    %assign/vec4 v0x5620660d6c00_0, 0;
    %load/vec4 v0x5620660fca40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %load/vec4 v0x5620660fc960_0;
    %assign/vec4 v0x5620660fc700_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660fc7e0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5620660fc8a0_0, 0;
T_1.6 ;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660d6c00_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
