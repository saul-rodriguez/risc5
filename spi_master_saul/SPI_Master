S_0x5620660dc180 .scope module, "SPI_Master_Pico_TB" "SPI_Master_Pico_TB" 2 12;
 .timescale -9 -12;
P_0x5620660d0600 .param/l "CLKS_PER_HALF_BIT" 0 2 16, +C4<00000000000000000000000000000101>;
P_0x5620660d0640 .param/real "CLK_PERIOD" 0 2 17, Cr<m7d00000000000000gfc7>; value=62.5000
L_0x5620660c4100 .functor BUFZ 1, v0x5620660d6c00_0, C4<0>, C4<0>, C4<0>;
v0x5620660fcc40_0 .var "Clks_per_half_bit", 11 0;
v0x5620660fcd40_0 .var "addr", 31 0;
v0x5620660fce00_0 .var "clk", 0 0;
v0x5620660fced0_0 .net "mem_ready", 0 0, L_0x5620660c4100;  1 drivers
v0x5620660fcfa0_0 .var "mem_valid", 0 0;
v0x5620660fd040_0 .var "resetn", 0 0;
v0x5620660fd110_0 .net "rx_data", 7 0, v0x5620660fc620_0;  1 drivers
v0x5620660fd1e0_0 .net "spi_master_ready", 0 0, v0x5620660d6c00_0;  1 drivers
v0x5620660fd2b0_0 .net "spi_master_tx_int_flag", 0 0, v0x5620660fc7e0_0;  1 drivers
v0x5620660fd410_0 .var "wdata", 7 0;
v0x5620660fd4e0_0 .var "wstrobe", 0 0;
S_0x5620660dc390 .scope task, "SendSingleByte" "SendSingleByte" 2 71, 2 71 0, S_0x5620660dc180;
 .timescale -9 -12;
v0x5620660d9550_0 .var "data", 7 0;
E_0x5620660cc5f0 .event posedge, v0x5620660fc7e0_0;
E_0x5620660cc760 .event posedge, v0x5620660db760_0;
TD_SPI_Master_Pico_TB.SendSingleByte ;
    %wait E_0x5620660cc760;
    %load/vec4 v0x5620660d9550_0;
    %assign/vec4 v0x5620660fd410_0, 0;
    %pushi/vec4 3402284734, 0, 32;
    %assign/vec4 v0x5620660fcd40_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5620660fcfa0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5620660fd4e0_0, 0;
    %wait E_0x5620660cc760;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5620660fd410_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5620660fcd40_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660fcfa0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5620660fd4e0_0, 0;
    %wait E_0x5620660cc5f0;
    %end;
S_0x5620660fbe00 .scope module, "spi" "SPI_master_pico" 2 54, 3 4 0, S_0x5620660dc180;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 32 "addr"
    .port_info 2 /INPUT 8 "wdata"
    .port_info 3 /INPUT 1 "wen"
    .port_info 4 /INPUT 1 "resetn"
    .port_info 5 /INPUT 1 "mem_valid"
    .port_info 6 /INPUT 1 "mem_ready"
    .port_info 7 /OUTPUT 1 "mem_port_ready"
    .port_info 8 /OUTPUT 8 "rx_data"
    .port_info 9 /OUTPUT 1 "tx_ready"
P_0x5620660fbff0 .param/l "ADDR" 0 3 17, C4<11001010110010101011111010111110>;
P_0x5620660fc030 .param/l "CLEAN_UP" 1 3 48, C4<11>;
P_0x5620660fc070 .param/l "IDLE" 1 3 46, C4<00>;
P_0x5620660fc0b0 .param/l "TX" 1 3 47, C4<01>;
P_0x5620660fc0f0 .param/l "WIDTH" 0 3 18, +C4<00000000000000000000000000001000>;
v0x5620660d9940_0 .var "SPI_state", 1 0;
v0x5620660d9d60_0 .net "addr", 31 0, v0x5620660fcd40_0;  1 drivers
v0x5620660db760_0 .net "clk", 0 0, v0x5620660fce00_0;  1 drivers
v0x5620660d6c00_0 .var "mem_port_ready", 0 0;
v0x5620660d7f40_0 .net "mem_ready", 0 0, L_0x5620660c4100;  alias, 1 drivers
v0x5620660d47d0_0 .net "mem_valid", 0 0, v0x5620660fcfa0_0;  1 drivers
v0x5620660fc560_0 .net "resetn", 0 0, v0x5620660fd040_0;  1 drivers
v0x5620660fc620_0 .var "rx_data", 7 0;
